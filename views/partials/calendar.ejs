<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      src="https://kit.fontawesome.com/211eaa680e.js"
      crossorigin="anonymous"
    ></script>
    <link rel="stylesheet" href="/styles.css">
    <title></title>
  </head>
  <body>
    <!-- calendar -->
    <div class="cal-aside-container">
      <main class="calendar-container">
        <section class="calendar">
          <div class="month">
            <div class="date">
              <h1></h1>
            </div>
            <section class="month-icons">
              <i class="fas fa-angle-left prev prev-month"></i>
              <i class="fas fa-angle-right next next-month"></i>
            </section>
          </div>
          <section class="calendar-days">
            <div>S</div>
            <div>M</div>
            <div>T</div>
            <div>W</div>
            <div>T</div>
            <div>F</div>
            <div>S</div>
          </section>
          <section class="calendar-dates">
            <div class="days"></div>
          </section>
        </section>
      </main>
      
      <!-- aside -->
      <%-include("aside.ejs")%>
      <%-include("eventsModal.ejs")%>
  </body>
</html>
